<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.ChatPlatformForPost" table="CHAT_PLATFORM_FOR_POST" schema="PUBLIC" catalog="PUBLIC">
        <composite-id mapped="true" class="model.ChatPlatformForPostPK">
            <key-property name="postId" column="POST_ID"/>
            <key-property name="chatPlatformId" column="CHAT_PLATFORM_ID"/>
        </composite-id>
        <many-to-one name="chatPlatformByChatPlatformId" class="model.ChatPlatform">
            <column name="CHAT_PLATFORM_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>