<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.GamePlatformForPost" table="GAME_PLATFORM_FOR_POST" schema="PUBLIC" catalog="PUBLIC">
        <composite-id mapped="true" class="model.GamePlatformForPostPK">
            <key-property name="postId" column="POST_ID"/>
            <key-property name="gamePlatformId" column="GAME_PLATFORM_ID"/>
        </composite-id>
    </class>
</hibernate-mapping>