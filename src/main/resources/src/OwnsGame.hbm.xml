<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.OwnsGame" table="OWNS_GAME" schema="PUBLIC" catalog="PUBLIC">
        <composite-id mapped="true" class="model.OwnsGamePK">
            <key-property name="ownerId" column="OWNER_ID"/>
            <key-property name="gameId" column="GAME_ID"/>
        </composite-id>
        <many-to-one name="gameByGameId" class="model.Game">
            <column name="GAME_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>