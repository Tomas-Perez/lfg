<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Group" table="GROUP" schema="PUBLIC" catalog="PUBLIC">
        <id name="groupId" column="GROUP_ID"/>
        <property name="slots" column="SLOTS"/>
        <many-to-one name="activityByActivityId" class="model.Activity">
            <column name="ACTIVITY_ID" not-null="true"/>
        </many-to-one>
        <many-to-one name="chatPlatformByChatPlatformId" class="model.ChatPlatform">
            <column name="CHAT_PLATFORM_ID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>